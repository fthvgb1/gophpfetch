{
    "name": "gophp/gophpfetch",
    "description": "A php http request extension on concurrence writing in golang, use FFI call",
    "keywords": [
        "FFI",
        "golang",
        "http",
        "request"
    ],
    "type": "library",
    "homepage": "https://github.com/fthvgb1/gophpfetch",
    "require": {
        "php": ">=7.4",
        "ext-ffi": "*",
        "ext-mbstring": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "GoPHP\\GoPHPFetch\\": "src/"
        }
    },
    "scripts": {
        "pre-package-install": [
            "GoPHP\\GoPHPFetch\\Script::checkAndDownloadExtension"
        ],
        "post-package-update": [
            "GoPHP\\GoPHPFetch\\Script::checkAndDownloadExtension"
        ]
    },
    "authors": [
        {
            "name": "xing",
            "email": "fthvgbx1@gmail.com"
        }
    ]
}
